{
    "Lua.runtime.version": "Lua 5.4",
    "Lua.runtime.special": {
        "set_targetdir":    "require",
        "add_headerfiles":  "require",
        "add_includedirs":  "require",
        "set_objectdir":    "require",
        "add_files":        "require",
        "includes":         "require",
        "add_configfiles":  "require"
    },
    "Lua.runtime.builtin": {
